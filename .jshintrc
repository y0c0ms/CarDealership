{
  "esversion": 8,
  "node": true,
  "strict": false,
  "globalstrict": false,
  "undef": true,
  "unused": false,
  "predef": [
    "require",
    "module",
    "exports",
    "console",
    "process",
    "global",
    "Buffer",
    "__dirname",
    "__filename"
  ],
  "asi": true,
